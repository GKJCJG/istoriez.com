{
    "version": "1.0.0",
    "name": "istoriez",
    "private": true,
    "main": "n/a",
    "scripts": {
        "start": "rm -rf publics && npm run clean && cross-env SASS_PATH=./node_modules GATSBY_PWA=true gatsby develop",
        "start-en": "rm -rf publics && npm run clean && cross-env SASS_PATH=./node_modules GATSBY_SITE_LANG=en GATSBY_PWA=true gatsby develop",
        "build": "npm run clean && cross-env SASS_PATH=./node_modules GATSBY_PWA=true gatsby build",
        "build-prefix": "npm run clean && cross-env SASS_PATH=./node_modules GATSBY_PATH_PREFIX=/sv GATSBY_PWA=true gatsby build --prefix-paths",
        "build-all": "rm -rf publics && mkdir publics && npm run build-prefix && cp -r public publics/sv && npm run clean && cross-env SASS_PATH=./node_modules GATSBY_SITE_LANG=en GATSBY_PWA=true gatsby build && cp -r publics/sv public/sv",
        "serve": "npm run build && cross-env SASS_PATH=./node_modules GATSBY_PWA=true gatsby serve",
        "serve-all": "npm run build-all && cross-env SASS_PATH=./node_modules GATSBY_PWA=true gatsby serve",
        "serve-en": "cross-env SASS_PATH=./node_modules GATSBY_SITE_LANG=en GATSBY_PWA=true gatsby build && cross-env SASS_PATH=./node_modules GATSBY_PWA=true gatsby serve",
        "clean": "gatsby clean",
        "mp3-en": "cross-env GATSBY_SITE_LANG=en node scripts/generateMp3s.js",
        "mp3": "node scripts/generateMp3s.js",
        "start-covers": "cross-env GATSBY_COVERS=true npm run start",
        "serve-covers": "cross-env GATSBY_COVERS=true npm run serve-all",
        "generate-covers": "cd packages/covers && npm run covers",
        "covers": "start-server-and-test serve-covers http://localhost:9000/images/ generate-covers"
    },
    "dependencies": {
        "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.4.2",
        "@google-cloud/text-to-speech": "^2.1.2",
        "cookieconsent": "3.1.1",
        "gatsby-plugin-offline": "3.0.34",
        "execa": "4.0.3",
        "front-matter": "4.0.2",
        "gatsby": "2.24.14",
        "gatsby-image": "2.4.14",
        "gatsby-link": "2.4.13",
        "gatsby-plugin-catch-links": "2.3.11",
        "gatsby-plugin-google-gtag": "2.1.10",
        "gatsby-plugin-layout": "1.3.10",
        "gatsby-plugin-manifest": "2.4.21",
        "gatsby-plugin-netlify": "2.3.11",
        "gatsby-plugin-react-helmet": "3.3.10",
        "gatsby-plugin-react-svg": "3.0.0",
        "gatsby-plugin-sass": "2.3.12",
        "gatsby-plugin-sharp": "2.6.23",
        "gatsby-plugin-sitemap": "2.4.11",
        "gatsby-remark-autolink-headers": "2.3.11",
        "gatsby-remark-copy-linked-files": "2.3.12",
        "gatsby-remark-images": "3.3.22",
        "gatsby-remark-relative-images": "0.3.0",
        "gatsby-source-filesystem": "2.3.23",
        "gatsby-transformer-remark": "2.8.27",
        "gatsby-transformer-sharp": "2.5.11",
        "local-storage": "2.0.0",
        "lodash": "4.17.19",
        "node-sass": "4.14.1",
        "normalize-scss": "7.0.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-helmet": "6.1.0",
        "react-intersection-observer": "8.26.2",
        "react-swipeable-views": "0.13.9",
        "remove-markdown": "0.3.0"
    },
    "devDependencies": {
        "@babel/core": "7.10.5",
        "@babel/plugin-proposal-class-properties": "7.10.4",
        "@babel/plugin-proposal-optional-chaining": "7.10.4",
        "@babel/preset-env": "7.10.4",
        "@babel/preset-react": "7.10.4",
        "autoprefixer": "9.8.5",
        "babel-core": "6.26.3",
        "babel-eslint": "10.1.0",
        "babel-jest": "24.9.0",
        "browserslist": "4.13.0",
        "classnames": "2.2.6",
        "concurrently": "5.0.2",
        "cross-env": "6.0.3",
        "eslint": "6.8.0",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-plugin-import": "2.19.1",
        "eslint-plugin-jest": "23.1.1",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.17.0",
        "eslint-watch": "6.0.1",
        "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
        "googleapis": "^33.0.0",
        "identity-obj-proxy": "3.0.0",
        "react-motion": "0.5.2",
        "react-test-renderer": "16.12.0",
        "react-transition-group": "1.2.1",
        "resize-observer-polyfill": "1.5.1",
        "start-server-and-test": "^1.11.2",
        "stylelint": "12.0.0",
        "stylelint-config-recommended-scss": "4.1.0",
        "stylelint-scss": "3.13.0"
    }
}
